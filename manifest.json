{
    "name": "PinnBorad",
    "version": "0.1.0",
    "description": "More than A Bookmark",
    "permissions": ["storage", "tabs","scripting","activeTab"],
    "host_permissions": ["https://*.youtube.com/*","https://*.twitter.com/*","https://*.linkedin.com/*","https://discord.com/*","https://github.com/*","https://web.whatsapp.com/"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://*.youtube.com/*","https://*.twitter.com/*","https://*.linkedin.com/*","https://discord.com/*","https://github.com/*","https://web.whatsapp.com/"],
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
            "assets/bookmark.png"
          ],
        "matches": ["https://*.youtube.com/*","https://*.twitter.com/*","https://*.linkedin.com/*","https://discord.com/*","https://github.com/*","https://web.whatsapp.com/*"]
      }
    ],
    "action": {
      "default_title": "PinnBorad",
      "default_popup": "/popup/popup.html"
    },
    "manifest_version": 3
  }
  